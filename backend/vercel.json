{
  "version": 2,
  "builds": [
      {
          "src": "backend/index.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/api/auth/(.*)",
          "dest": "/backend/Routes/Auth.js"
      },
      {
          "src": "/api/checkout/(.*)",
          "dest": "/backend/Routes/checkout.js"
      },
      {
          "src": "/(.*)",
          "dest": "/backend/index.js"
      }
  ]
}
