{
  "version": 2,
  "builds": [
    {
      "src": "backend/Routes/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "backend/middleware/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/Routes/$1.js"
    },
    {
      "src": "/middleware/(.*)",
      "dest": "/backend/middleware/$1.js"
    }
  ]
}
